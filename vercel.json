{
  "version": 2,
  "builds": [
    {
      "src": "bin/www",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/projects(/.*)?",
      "dest": "/api/projects"
    },
    {
      "src": "/skills(/.*)?",
      "dest": "/api/skills"
    },
    {
      "src": "/users(/.*)?",
      "dest": "/api/users"
    },
    {
      "src": "/bootcamps(/.*)?",
      "dest": "/api/bootcamps"
    },
    {
      "src": "/auth(/.*)?",
      "dest": "/api/auth"
    },
    {
      "src": "/form",
      "dest": "/"
    },
    {
      "src": "/login",
      "dest": "/"
    },
    {
      "src": "/index",
      "dest": "./bin/www"
    },
    {
      "src": "/dashboard",
      "dest": "/api/dashboard"
    },
    {
      "src": "/(.*)",
      "dest": "/public/(.*)"
    }
  ],
  "env": {
    "POSTGRES_URL" : "postgres://default:VEgKu8O3JLid@ep-winter-mountain-a4k1x5q7-pooler.us-east-1.aws.neon.tech:5432/verceldb?sslmode=require",
    "NODE_ENV": "production"
  }
}
